<h2><%= l(:label_board_new) %></h2>

<% labelled_tabular_form_for :board, @board, :url => {:action => 'new'} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <div class="gt-table-buttons">
      <%= submit_tag l(:button_create), :disable_with => l(:button_working) %>
  </div>
<% end %>
