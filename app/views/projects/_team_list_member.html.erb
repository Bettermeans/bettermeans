<tr id="<%= member.user.login %>" class="<%= cycle 'odd', 'even' %> member <%= member.user.name %>">
<td width="10px">  <%= avatar(member.user, :size => "64") %></td>
<td class="<%= member.user.class.name.downcase %>"><%= link_to_user member.user %></td>
<td class="roles">
  <span id="member-<%= member.id %>-roles"><%=h member.roles.sort.collect(&:to_s).join(', ') %></span>
</td>
<td class="points">
  <%= nomination_links(member,@project) %>
</td>
</tr>
